<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.dylowen.loopbackalias</string>
    <key>RunAtLoad</key>
    <true/>
    <key>ProgramArguments</key>
    <array>
      <string>{install_location}/loopback_alias.sh</string>
    </array>
    <key>StandardOutPath</key>
    <string>{install_location}/stdout.log</string>
    <key>StandardErrorPath</key>
    <string>{install_location}/stderr.log</string>
    <key>Debug</key>
    <false/>
</dict>
</plist>